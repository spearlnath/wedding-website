{
    "ci": {
      "collect": {
        "staticDistDir": "./build"
      },
      "upload": {
        "target": "temporary-public-storage"
      },
      "assert": {
        "preset": "lighthouse:recommended"
      }
    },
    "assert": {
      "assertions": {
        "categories:performance": "off",
        "categories:accessibility": "off",
        "categories:best-practices": "off",
        "categories:seo": "off",
        "categories:pwa": "off"
      }
    }
}