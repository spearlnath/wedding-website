{
    "ci": {
      "collect": {
        "url": ["http://localhost/"], 
        "startServerCommand": "http-server ./build/ -p 4000 -g",
        "startServerReadyPattern": "Available on",
        "numberOfRuns": 1
      },
      "upload": {
        "target": "temporary-public-storage"
      },
      "assert": {
        "preset": "lighthouse:recommended"
      }
    }
}